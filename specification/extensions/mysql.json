{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "definitions": {
        "MysqlDataSetExtension": {
            "type": "object",
            "properties": {
                "engine": {
                    "type": "string"
                },
                "version": {
                    "type": "integer"
                },
                "row_format": {
                    "type": "string"
                },
                "avg_row_length": {
                    "type": "integer"
                },
                "data_length": {
                    "type": "integer"
                },
                "max_data_length": {
                    "type": "integer"
                },
                "index_length": {
                    "type": "integer"
                },
                "data_free": {
                    "type": "integer"
                },
                "auto_increment": {
                    "type": "integer"
                },
                "check_time": {
                    "type": "string",
                    "format": "date-time"
                },
                "table_collation": {
                    "type": "string"
                },
                "checksum": {
                    "type": "integer"
                },
                "create_options": {
                    "type": "string"
                }
            }
        },
        "MysqlDataSetFieldExtension": {
            "type": "object",
            "properties": {
                "ordinal_position": {
                    "type": "integer"
                },
                "character_maximum_length": {
                    "type": "integer"
                },
                "character_octet_length": {
                    "type": "integer"
                },
                "numeric_precision": {
                    "type": "integer"
                },
                "numeric_scale": {
                    "type": "integer"
                },
                "datetime_precision": {
                    "type": "integer"
                },
                "character_set_name": {
                    "type": "string"
                },
                "collation_name": {
                    "type": "string"
                },
                "column_key": {
                    "type": "string"
                },
                "extra": {
                    "type": "string"
                },
                "privileges": {
                    "type": "string"
                },
                "generation_expression": {
                    "type": "string"
                }
            }
        }
    }
}
